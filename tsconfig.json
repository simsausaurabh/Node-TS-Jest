{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": false,
    "declaration": true,
    "inlineSources": true,
    "types": ["node", "jasmine"]
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "/node_modules/@types/**/*"
  ]
}